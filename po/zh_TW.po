msgid "F Virtual Window Manager"
msgstr ""

#: ../switchdesk.glade.h:15
msgid "IceWM"
msgstr ""

#: ../switchdesk.glade.h:16
msgid "Fluxbox"
msgstr ""

#: ../switchdesk.glade.h:17
msgid "System default"
msgstr "系统默认"

#: ../switchdesk.glade.h:18
msgid "Change only applies to current display"
msgstr "變更只適用於本次的桌面顯示"
